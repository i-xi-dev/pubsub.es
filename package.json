{
  "name": "@i-xi-dev/pubsub",
  "version": "0.0.2",
  "description": "A JavaScript Pub/Sub Broker.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "node": "./node/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "lint": "eslint --ext ts --fix ./src/",
    "test": "mocha test/node/ --recursive",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/i-xi-dev/pubsub.es.git"
  },
  "keywords": [
    "pubsub",
    "browser",
    "deno",
    "nodejs"
  ],
  "author": "i-xi-dev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/i-xi-dev/pubsub.es/issues"
  },
  "homepage": "https://github.com/i-xi-dev/pubsub.es#readme",
  "files": [
    "dist",
    "node"
  ],
  "engines": {
    "node": ">=15.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4"
  },
  "publishConfig": {
    "access": "public"
  }
